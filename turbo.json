{
	"$schema": "https://turborepo.com/schema.json",
	"ui": "stream",
	"tasks": {
		"build": {
			"dependsOn": [
				"^build"
			],
			"inputs": [
				"$TURBO_DEFAULT$",
				".env*"
			],
			"outputs": [
				"dist/**",
				"bin/**"
			]
		},
		"test": {
			"dependsOn": [
				"^build"
			],
			"outputs": [
				"coverage.out",
				"coverage.html"
			]
		},
		"lint": {
			"dependsOn": [
				"^lint"
			]
		},
		"check-types": {
			"dependsOn": [
				"^build",
				"^check-types"
			]
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
