{
  "name": "@sabaipics/face-recognition",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "description": "SabaiFace: Self-hosted face recognition service compatible with AWS Rekognition API",
  "exports": {
    ".": {
      "types": "./client/src/index.ts",
      "import": "./client/src/index.ts"
    },
    "./server": {
      "types": "./src/server.ts",
      "import": "./src/server.ts"
    }
  },
  "scripts": {
    "models:download": "tsx scripts/download-models.ts",
    "postinstall": "tsx scripts/download-models.ts",
    "dev": "tsx watch src/server.ts",
    "start": "tsx src/server.ts",
    "build": "tsc",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:run": "vitest run",
    "test:images": "tsx tests/manual/test-with-images.ts",
    "test:thresholds": "tsx tests/manual/test-multiple-thresholds.ts",
    "test:compare": "tsx tests/manual/test-compare-providers.ts",
    "test:detection": "tsx tests/manual/test-detection-comparison.ts",
    "test:api": "tsx tests/manual/test-api-with-images.ts",
    "test:indexing-compare": "tsx tests/manual/test-indexing-comparison.ts",
    "test:model-compare": "tsx tests/manual/test-model-comparison.ts",
    "test:optimize": "tsx tests/manual/test-optimization.ts",
    "test:aws-vs-optimized": "tsx tests/manual/test-aws-vs-optimized.ts",
    "label": "tsx tests/fixtures/images/label-server.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@aws-sdk/client-rekognition": "^3.0.0",
    "@hono/node-server": "^1.8.0",
    "@hono/zod-validator": "^0.2.0",
    "@neondatabase/serverless": "^0.10.0",
    "@sabaipics/db": "workspace:*",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.0",
    "hono": "^4.0.0",
    "neverthrow": "^6.0.0",
    "zod": "^3.22.0"
  },
  "optionalDependencies": {
    "@tensorflow/tfjs-node": "^4.20.0",
    "@vladmandic/face-api": "^1.7.12",
    "canvas": "^2.11.0",
    "vectordb": "^0.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "drizzle-kit": "^0.31.8",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
