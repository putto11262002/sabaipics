{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"lib": ["ESNext", "DOM"],
		"jsx": "react-jsx",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"allowJs": true,
		"noEmit": true,
		"strict": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["src/**/*"]
}
